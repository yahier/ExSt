<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="register_label_tip">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/theme_black_a</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="register_input_hint">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/theme_black_a</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>

    </style>

    <style name="register_input_value">
        <item name="android:padding">10dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black7</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="lin_group">
        <item name="android:layout_marginTop">12dp</item>
    </style>

    <style name="register_img">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">100dp</item>
    </style>
</resources>