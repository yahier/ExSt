<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="im_list_item">
        <item name="android:background">@color/white</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="im_list_img">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
    </style>

    <style name="im_list_img_45">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
    </style>

    <style name="im_list_name1">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/gray1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="im_list_name2">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/theme_black_a</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="im_list_name_a19c98">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/gray_a5</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="im_invite_text">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/theme_black_a</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="im_group_window_text">
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black7</item>
    </style>

</resources>